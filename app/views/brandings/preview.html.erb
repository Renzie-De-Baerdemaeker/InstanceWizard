<div class="bg-purple-400">
<div class="flex items-center h-screen w-screen bg-gray-100" data-controller="preview">
   <div class="self-center bg-white m-6 card" data-theme="light">
      <h1 class="card-title self-center mt-6">Choose branding colors</h1>
      <%= form_with(model: @branding, data: {controller: "preview_controller"}) do |form| %>
      <div data-controller="preview", class="flex flex-col m-10 w-50">
         <div class="flex items-center mb-4">
            <%= form.color_field :primary_color, data: { preview_target: "color", variable: "--color-primary", action: "input->preview#update"}, class: "rounded-full w-10 h-10 cursor-pointer mr-4" %>
            <%= form.label :primary%>
         </div>
         <div  class="flex items-center mb-4">
            <%= form.color_field :secondary_color_light, data: { preview_target: "color", variable: "--color-secondary", action: "input->preview#update"}, class: "rounded-full w-10 h-10 cursor-pointer mr-4" %>
            <%= form.label :secondary_light %>
         </div>
         <div class="flex items-center mb-4"> 
            <%= form.color_field :secondary_color_dark, data: { preview_target: "color", variable: "--color-secondary", action: "input->preview#update"}, class: "rounded-full w-10 h-10 cursor-pointer mr-4" %>
            <%= form.label :secondary_dark %>
         </div>
         <div class="flex items-center">
            <%= form.color_field :accent_color, data: { preview_target: "color", variable: "--color-accent", action: "input->preview#update"}, class: "rounded-full w-10 h-10 cursor-pointer mr-4" %>
            <%= form.label :accent %>
         </div>
      </div>
      <div class="flex justify-center items-center mb-6">
         <button type="button" class="mr-2 cursor-pointer" data-action="click->preview#showLogin">Login</button>
         |
         <button type="button" class="mr-2 ml-2 cursor-pointer" data-action="click->preview#showOverview">Overview</button>
         |
         <%# Light and dark mode %>
         <label class="swap swap-rotate ml-2">
            <!-- this hidden checkbox controls the state -->
            <input type="checkbox" data-action="click->preview#toggleTheme" data-variable="--color-secondary"/>
            <!-- sun icon -->
            <svg
               class="swap-off h-10 w-10 fill-current"
               xmlns="http://www.w3.org/2000/svg"
               viewBox="0 0 24 24">
               <path
                  d="M5.64,17l-.71.71a1,1,0,0,0,0,1.41,1,1,0,0,0,1.41,0l.71-.71A1,1,0,0,0,5.64,17ZM5,12a1,1,0,0,0-1-1H3a1,1,0,0,0,0,2H4A1,1,0,0,0,5,12Zm7-7a1,1,0,0,0,1-1V3a1,1,0,0,0-2,0V4A1,1,0,0,0,12,5ZM5.64,7.05a1,1,0,0,0,.7.29,1,1,0,0,0,.71-.29,1,1,0,0,0,0-1.41l-.71-.71A1,1,0,0,0,4.93,6.34Zm12,.29a1,1,0,0,0,.7-.29l.71-.71a1,1,0,1,0-1.41-1.41L17,5.64a1,1,0,0,0,0,1.41A1,1,0,0,0,17.66,7.34ZM21,11H20a1,1,0,0,0,0,2h1a1,1,0,0,0,0-2Zm-9,8a1,1,0,0,0-1,1v1a1,1,0,0,0,2,0V20A1,1,0,0,0,12,19ZM18.36,17A1,1,0,0,0,17,18.36l.71.71a1,1,0,0,0,1.41,0,1,1,0,0,0,0-1.41ZM12,6.5A5.5,5.5,0,1,0,17.5,12,5.51,5.51,0,0,0,12,6.5Zm0,9A3.5,3.5,0,1,1,15.5,12,3.5,3.5,0,0,1,12,15.5Z" />
            </svg>
